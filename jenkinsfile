pipeline {
    agent any

    stages {
        stage('Hello') {
            steps {
                script {
                    echo 'Hello World'
                    echo "${J_V}"
                    echo "===>>> ${env.data}"
                    echo "================>>>>>>>> ${env.FOO}"
                    echo "------------------"
                    echo "===>>> ${params.data}"
                    def json = readJSON text: env.data
                    println("JSON:\n${json}")
                }

            }
        }
    }
}

